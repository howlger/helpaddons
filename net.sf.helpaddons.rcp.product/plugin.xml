<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="net.sf.helpaddons.rcp.product.application"
            >
         <property
               name="appName"
               >
         </property>
         <property
               name="windowImages"
               value="icons/help16.gif,icons/help32.gif,icons/help48.gif,icons/help64.gif,icons/help128.gif">
         </property>
      </product>
   </extension>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="*"
            icon="icons/help16.gif"
            thread="main"
            visible="true">
         <run
               class="net.sf.helpaddons.rcp.product.internal.HelpRcpApplication">
         </run>
      </application>
   </extension>

</plugin>
